docker cp ~/.ssh netberry_web:/var/www/
docker cp ~/.gitconfig netberry_web:/var/www/

docker exec -it netberry_db zsh
mysql -uroot -psecret
create database db

docker exec -it -u app netberry_web zsh
composer install
php artisan migrate
